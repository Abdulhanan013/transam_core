.row
  .col-md-12
    %h4.pull-left.media-heading
      = link_to "[#{search_result.organization}] #{search_result.object_class}: #{search_result.summary}", eval(search_result.name)
    %p.text-muted.pull-right
      = search_result.object_key
  .col-md-12
    %p.search-results-text= sanitize(search_result.search_text.truncate(128))
