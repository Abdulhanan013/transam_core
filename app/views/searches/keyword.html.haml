.col-lg-8.col-md-8.col-sm-10.col-xs-12.col-lg-offset-2.col-md-offset-2.col-sm-offset-1

  = render 'dashboards/search'

  %fieldset
    %legend.panel-legend= "#{@keyword_search_results.count} found."

    %table.table.table-condensed.table-hover
      - @keyword_search_results.each do |search_result|
        = render :partial => 'keyword_search_result_detail', :locals => {:search_result => search_result }
