<script type="text/javascript">

	$(document).ready(function() {

		transam.enable_date_pickers();			
		transam.make_same_height('.form_part');

	    // Page Load Events    
	    $('#spinner').hide();
	    
	    // Search Form AJAX event handling
	    $("form").on("ajax:send", function(e, data, status, xhr) {
			  $("#asset_search_form").collapse('hide');
			  $('#results_table').hide();
			  $('#spinner').show();
			}).on("ajax:complete", function(e, xhr, status, error) {
			  $('#spinner').hide();
			});

	
	});
		
</script>
