<!-- Add any globally applicable javascript here -->
<script type="text/javascript">
		
	$(document).ready(function() {
		
		// install click handlers for navigable objects
		transam.install_quick_link_handlers();
		
		// Render any info icons as popovers
		transam.enable_info_popups('.info_icon');

		// Enable any date pickers etc
		transam.enable_date_pickers();
	
		// Check for any message divs and fade them out
		elem = $('#message');
		if (elem) {
			elem.fadeOut(5000);
		}	
	});
		  		
</script>
