%table.table.table-hover#datatable
  %thead
    %tr
      %th.left Name
      %th.left Address
      				
  %tbody
    - @vendors.each do |v|      
      %tr.popup-details{:data => {:action_path => vendor_path(v)}}
        %td.left= v.name
        %td.left= format_as_address(v)

= render 'shared/datatable_javascript'
