%th.center{:data => {:field => "description",   :sortable => 'false', :searchable => 'false'}} Description
%th.center{:data => {:field => "parent",        :sortable => 'true',  :searchable => 'true'}} Parent
%th.center{:data => {:field => "service_status",        :sortable => 'true',  :searchable => 'true'}} Status
%th.center{:data => {:field => "age",           :sortable => 'true',  :searchable => 'true'}} Age
%th.center{:data => {:field => "condition_rating",     :sortable => 'true',  :searchable => 'true'}} Condition
%th.center{:data => {:field => "scheduled_rehabilitation_year",    :sortable => 'true',  :searchable => 'true'}} Rehab
%th.center{:data => {:field => "scheduled_replacement_year",  :sortable => 'true',  :searchable => 'true'}} Replace
