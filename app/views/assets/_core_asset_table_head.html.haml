- if @organization_list.count > 1
  %th.center.nowrap{:data => {:field => "org",           :sortable => 'true',  :searchable => 'true'}} Agency

%th.center{:data => {:field => "asset_subtype",       :sortable => 'true',  :searchable => 'true'}} Type
