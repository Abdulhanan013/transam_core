<div class="row-fluid">
	<div class="span12">
		<table id="datatable" class="table table-condensed table-striped table-hover table-bordered">
			<thead>
				<tr>
					<th class="center">Name</th>
					<th class="center">Created On</th>
				</tr>
			</thead>
			<tbody>
				<% @notes.each do |a| %>
					<tr data-action-path='<%= inventory_note_path(@asset, a) %>' >
						<td class="left"><%= a.comments %></td>
						<td class="right"><%= format_date_time(a.created_at) %></td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>

<%= render 'shared/datatable_javascript' %>
